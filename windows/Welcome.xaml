<Window x:Class="GameofKingdom.windows.Welcome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameofKingdom.windows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="GameOfKingdom" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden"/>
        <Grid.Background>
            <ImageBrush ImageSource="\images\background_image.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
    </Grid>
</Window>

<UserControl x:Class="GameofKingdom.windows.tmp.WelcomeWindowPages.LicencePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GameofKingdom.windows.tmp.WelcomeWindowPages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="32" Grid.Row="1" FontWeight="Bold">Zgoda Licencyjna</TextBlock>
        <TextBlock Background="AliceBlue" Name="instruction" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="18" Grid.Row="2" MaxWidth="500" TextWrapping="Wrap"></TextBlock>
        <Button Grid.Row="3" Width="175" FontSize="22" Content="Wróć do menu" HorizontalAlignment="Center" Click="Btn_return"/>
    </Grid>
</UserControl>
